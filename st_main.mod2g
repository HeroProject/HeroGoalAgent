use dialogflow as knowledge.
use script as knowledge.
use dialogflow as actionspec.
use 'stateLogic/performStateActions' as module.

module st_main {
	% Doing things
	if bel(currentTopic(T), currentState(S), start, not(waitingForEvent(_))) then 
	{
		if bel(not(audio(T, S, recorded, _))) then performStateActions(T, S).
		
		if bel(audio(T, S, recorded, Source), nth0(0, Source, Tsource), nth0(1, Source, Ssource), audioRecording(Tsource, Ssource, _))
			then performStateActions(T, S).
	}
}
	
	