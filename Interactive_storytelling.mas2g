use 'DialogFlow-0.0.1-SNAPSHOT-shaded.jar' as environment with
		server='localhost',
		flowkey='dialogflowagent.json',
		flowagent='mikeagent-dsvpjs',
		flowlang='nl-NL',
		recordaudio='yes'.
		
define storytelling as agent {
	use st_init as init.
	use st_event as event.
	use st_main as main.
}

launchpolicy{
	when * launch storytelling.
}